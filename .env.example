# Database Configuration
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=change_this_password
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=change_this_password

# n8n Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change_this_admin_password

# n8n Security
# Generate a strong encryption key (32+ characters)
N8N_ENCRYPTION_KEY=change_this_to_a_very_long_random_string

# External API Keys
# OpenAI API key for AI-powered content summarization
OPENAI_API_KEY=your_openai_api_key_here

# Claude API key for AI processing (alternative to OpenAI)
CLAUDE_API_KEY=your_claude_api_key_here

# n8n OpenAI Credential ID (OPTIONAL - only needed for custom workflows)
# The production workflow handles credentials automatically through n8n UI
# OPENAI_CREDENTIAL_ID=your_n8n_openai_credential_id

# GitHub API token (for accessing releases and repositories)
GITHUB_TOKEN=your_github_token_here

# Slack Webhook URL (for posting messages)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Dev.to API key (optional, for accessing dev.to articles)
DEVTO_API_KEY=your_devto_api_key_here

# Twitter/X API credentials (optional, if you want to include Twitter)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Obsidian configuration (will be configured later)
OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault
OBSIDIAN_API_KEY=your_obsidian_api_key_if_needed

# Timezone configuration
TZ=Europe/Warsaw
GENERIC_TIMEZONE=Europe/Warsaw

# Project configuration
PROJECT_NAME=Vue-Nuxt News Collector
COLLECTION_FREQUENCY=weekly